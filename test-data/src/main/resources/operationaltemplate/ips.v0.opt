              <items id="description">Method by which the baby was delivered.</items>
              <items id="text">Mode of birth</items>
            </term_definitions>
            <term_definitions code="at0041">
              <items id="description">Normal, non-instrumental vaginal delivery.</items>
              <items id="text">Unassisted</items>
            </term_definitions>
            <term_definitions code="at0042">
              <items id="description">Vaginal delivery assisted by the use of vacuum extraction.</items>
              <items id="text">Vacuum extraction</items>
            </term_definitions>
            <term_definitions code="at0043">
              <items id="description">Vaginal delivery assisted by the use of lift-out forceps.</items>
              <items id="text">Lift-out forceps</items>
            </term_definitions>
            <term_definitions code="at0044">
              <items id="description">Vaginal delivery assisted by the use of low forceps.</items>
              <items id="text">Low forceps</items>
            </term_definitions>
