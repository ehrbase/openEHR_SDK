                <term_definitions code="at0006">
                    <items id="text">Beliebiges Ereignis</items>
                    <items id="description">Standardmiger, nicht nher beschriebener Zeitpunkt oder Intervall Ereignis welches in einem Template oder bei der Anwendung genauer definiert werden kann.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">state structure</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Position</items>
                    <items id="description">Die Position der untersuchten Person whrend der Messung.</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="text">Listenstruktur</items>
                    <items id="description">Listenstruktur</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Manschettengre</items>
                    <items id="description">Die Gre der Manschette, die zur Blutdruckmessung benutzt wurde.</items>
                    <items id="comment">Perloff D, Grim C, Flack J, Frohlich ED, Hill M, McDonald M, Morgenstern BZ. Human blood pressure determination by sphygmomanometry. Circulation 1993;88;2460-2470.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">Stelle der Messung</items>
                    <items id="description">Einfache Krperstelle an der der Blutdruck gemessen wurde.</items>
                </term_definitions>
                <term_definitions code="at0015">
                    <items id="text">Oberschenkel eines Erwachsenen</items>
                    <items id="description">Eine Manschette fr den Oberschenkel eines Erwachsenen - ca. 20cm x 42cm.</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">Groer Erwachsener</items>
                    <items id="description">Eine Manschette fr Erwachsene mit greren Armen - ca. 16cm x 38cm.</items>
                </term_definitions>
                <term_definitions code="at0017">
                    <items id="text">Erwachsener</items>
                    <items id="description">Eine Standard-Manschette fr einen Erwachsenen - ca. 13cm x 30cm.</items>
                </term_definitions>
                <term_definitions code="at0025">
                    <items id="text">Rechter Arm</items>
                    <items id="description">Der rechte Arm der Person.</items>
                </term_definitions>
                <term_definitions code="at0026">
                    <items id="text">Linker Arm</items>
                    <items id="description">Der linke Arm der Person.</items>
                </term_definitions>
                <term_definitions code="at0027">
                    <items id="text">Rechter Oberschenkel</items>
                    <items id="description">Der rechte Oberschenkel der Person.</items>
                </term_definitions>
                <term_definitions code="at0028">
                    <items id="text">Linker Oberschenkel</items>
                    <items id="description">Der linke Oberschenkel der Person.</items>
                </term_definitions>
                <term_definitions code="at0033">
                    <items id="text">Kommentar</items>
                    <items id="description">Zustzlicher Freitext ber die Messung, der nicht in anderen Felder erfasst werden konnte.</items>
                </term_definitions>
                <term_definitions code="at1000">
                    <items id="text">Stehend</items>
                    <items id="description">Stehend zum Zeitpunkt der Blutdruckmessung.</items>
                </term_definitions>
                <term_definitions code="at1001">
                    <items id="text">Sitzend</items>
                    <items id="description">Sitzend zum Zeitpunkt der Blutdruckmessung (z.B. auf einem Bett oder Stuhl).</items>
                </term_definitions>
                <term_definitions code="at1002">
                    <items id="text">Zurckgelehnt</items>
                    <items id="description">Patient zurckgelehnt zum Zeitpunkt der Blutdruckmessung.</items>
                </term_definitions>
                <term_definitions code="at1003">
                    <items id="text">Liegend</items>
                    <items id="description">Flach liegend zum Zeitpunkt der Blutdruckmessung.</items>
                </term_definitions>
                <term_definitions code="at1005">
                    <items id="text">Neigung</items>
                    <items id="description">Die kraniokaudale Neigung der Oberflche auf der die Person zum Zeitpunkt der Messung liegt.</items>
                </term_definitions>
                <term_definitions code="at1006">
                    <items id="text">Mittlerer arterieller Druck</items>
                    <items id="description">Der mittlerer arterielle Druck ber den gesamten Verlauf der Konktraktions- und Entspannungsphase des Herzens.</items>
                </term_definitions>
                <term_definitions code="at1007">
                    <items id="text">Pulsdruck</items>
                    <items id="description">Der Abstand zwischen dem systolischen und dem diastolischen Blutdruckwert.</items>
                </term_definitions>
                <term_definitions code="at1008">
                    <items id="text">Kleiner Erwachsener</items>
                    <items id="description">Eine Manschette fr einen kleinen Erwachsenen - ca. 10cm x 24cm.</items>
                </term_definitions>
                <term_definitions code="at1009">
                    <items id="text">Pdiatrisch/Kind</items>
                    <items id="description">Eine Manschette fr ein Kind oder auch einen Erwachsenen mit einem schmalen Arm - 8cm x 21cm.</items>
                </term_definitions>
                <term_definitions code="at1010">
                    <items id="text">Diastolischer Endpunkt</items>
                    <items id="description">Dient der Dokumentation des Korotkoff-Gerusches, das verwendet wird, um bei auskultarischer Messung den diastolischen Blutdruck zu bestimmen.</items>
                </term_definitions>
                <term_definitions code="at1011">
                    <items id="text">Phase IV</items>
                    <items id="description">Das 4. Korotkoff-Gerusch - Die Gerusche klingen pltzlich gedmpft.</items>
                </term_definitions>
                <term_definitions code="at1012">
                    <items id="text">Phase V</items>
                    <items id="description">Das 5. Korotkoff-Gerusch - die Gerusche verschwinden vllig whrend der Manschettendruck unter den diastolischen Blutdruck fllt.</items>
                </term_definitions>
                <term_definitions code="at1014">
                    <items id="text">Nach links geneigt liegend</items>
                    <items id="description">Flach liegend mit seitlicher Neigung, normalerweise zur linken Seite. Hufig verwendet im letzten Drittel eine Schwangerschaft, um aorto-cavale Kompression zu vermeiden.</items>
                </term_definitions>
                <term_definitions code="at1018">
                    <items id="text">Kleinkind</items>
                    <items id="description">Eine Manschette fr Kleinkinder und Suglinge - ca. 5cm x 15cm.</items>
                </term_definitions>
                <term_definitions code="at1019">
                    <items id="text">Neonatal</items>
                    <items id="description">Eine Manschette fr Neugeborene mit passender Gre fr die Reife und das Geburtsgewicht des Neugeborenen.</items>
                </term_definitions>
                <term_definitions code="at1020">
                    <items id="text">Rechtes Handgelenk</items>
                    <items id="description">Das rechte Handgelenk der Person.</items>
                </term_definitions>
                <term_definitions code="at1021">
                    <items id="text">Linkes Handgelenk</items>
                    <items id="description">Das linke Handgelenk der Person.</items>
                </term_definitions>
                <term_definitions code="at1025">
                    <items id="text">Gert</items>
                    <items id="description">Details ber das Sphygmomanometer oder ein anderes Gert, dass zur Blutdruckmessung verwendet wird.</items>
                </term_definitions>
                <term_definitions code="at1026">
                    <items id="text">Rechtes Fugelenk</items>
                    <items id="description">Das rechte Fugelenk der Person.</items>
                </term_definitions>
                <term_definitions code="at1030">
                    <items id="text">Anstrengung</items>
                    <items id="description">Details ber physische Aktivitten zur Zeit der Blutdruckmessung.</items>
                </term_definitions>
                <term_definitions code="at1031">
                    <items id="text">Linkes Fugelenk</items>
                    <items id="description">Das linke Fugelenk der Person</items>
                </term_definitions>
                <term_definitions code="at1032">
                    <items id="text">Finger</items>
                    <items id="description">Ein Finger des Individuums.</items>
                </term_definitions>
                <term_definitions code="at1035">
                    <items id="text">Methode</items>
                    <items id="description">Methode der Messung des Blutdrucks.</items>
                </term_definitions>
                <term_definitions code="at1036">
                    <items id="text">Auskultation</items>
                    <items id="description">Auskulatorische Messung unter Benutzung eines Stethoskops und der Korotkoff-Gerusche.</items>
                </term_definitions>
                <term_definitions code="at1037">
                    <items id="text">Palpation</items>
                    <items id="description">Palpatorische Messung, normalerweise an den brachialen oder radialen Arterien.</items>
                </term_definitions>
                <term_definitions code="at1038">
                    <items id="text">Formel fr mittleren arterieller Druck</items>
                    <items id="description">Die Formel die ggf. verwendet wurde, um den mittleren arteriellen Druck zu berechnen.</items>
                </term_definitions>
                <term_definitions code="at1039">
                    <items id="text">Machine</items>
                    <items id="description">Messung durch eine Blutdruckmaschine.</items>
                </term_definitions>
                <term_definitions code="at1040">
                    <items id="text">Invasiv</items>
                    <items id="description">Invasive Messung des Blutdrucks innerhalb eines Gefes.</items>
                </term_definitions>
                <term_definitions code="at1042">
                    <items id="text">24 Stunden Durchschnitt</items>
                    <items id="description">Schtzung des durchschnittlichen Blutdrucks ber einen 24-stndigen Zeitraum.</items>
                </term_definitions>
                <term_definitions code="at1043">
                    <items id="text">Schlafzustand</items>
                    <items id="description">Schlafzustand - untersttzt die Auswertung von 24-stndigen ambulanten Aufzeichnungen des Blutdrucks.</items>
                </term_definitions>
                <term_definitions code="at1044">
                    <items id="text">Aufmerksam und wach</items>
                    <items id="description">Die untersuchte Person ist bei vollem Bewusstsein.</items>
                </term_definitions>
                <term_definitions code="at1045">
                    <items id="text">Schlafend</items>
                    <items id="description">Die untersuchte Person schlft.</items>
                </term_definitions>
                <term_definitions code="at1051">
                    <items id="text">Zeh</items>
                    <items id="description">Ein Zeh des Individuums.</items>
                </term_definitions>
                <term_definitions code="at1052">
                    <items id="text">Einflussfaktoren</items>
                    <items id="description">Kommentar und Aufzeichung anderer Faktoren die ggf. zu dem Ergebnis der Blutdruckmessung beitragen knnen. Die kann z.B. bei belastenden Situationen der Fall sein (z.B. sog. Weikittelhypertonie, Schmerzen, Fieber, nderungen im atmosphrischen Druck).</items>
                </term_definitions>
                <term_definitions code="at1053">
                    <items id="text">Intraarteriell</items>
                    <items id="description">Invasive Messung mittels Transducer ber einen arteriellen Zugang.</items>
                </term_definitions>
                <term_definitions code="at1054">
                    <items id="text">Formel fr systolischen Druck</items>
                    <items id="description">Formel, die benutzt wurde, um den systolischen Druck vom mittleren arteriellen Druck zu berechnen (wenn erfasst).</items>
                </term_definitions>
                <term_definitions code="at1055">
                    <items id="text">Formel fr diastolischen Druck</items>
                    <items id="description">Formel, die benutzt wurde, um den distolischen Druck vom mittleren arteriellen Druck zu berechnen (wenn erfasst).</items>
                </term_definitions>
                <term_definitions code="at1056">
                    <items id="text">Furcken</items>
                    <items id="description">Furcken des Individuums.</items>
                </term_definitions>
                <term_definitions code="at1057">
                    <items id="text">Strukturierte Stelle der Messung</items>
                    <items id="description">Strukturierte Krperstelle an der der Blutdruck gemessen wurde.</items>
                </term_definitions>
                <term_definitions code="at1058">
                    <items id="text">Erweiterung</items>
                    <items id="description">Zustzliche Information, die fr die Erfassung des lokalen Kontexts oder fr die Anpassung an andere Referenzmodelle/Formalismen bentigt wird.</items>
                    <items id="comment">Zum Beispiel: Informationen bzgl. der lokalen Krankenhausabteilung oder zustzliche Metadata zur Anpassung an entsprechende FHIR oder CIMI Gegenstcke.</items>
                </term_definitions>
                <term_definitions code="at1059">
                    <items id="text">Klinische Interpretation</items>
                    <items id="description">Einzelnes Wort, Ausdruck oder Kurzbeschreibung der klinischen Bedeutung und Aussagekraft der Blutdruckmessung.</items>
                </term_definitions>
                <term_definitions code="at0000">
                    <items id="text">Blutdruck</items>
                    <items id="description">Die lokale Messung des arteriellen Blutdrucks als Surrogat fr den arteriellen Druck in der systemischen Zirkulation.</items>
                    <items id="comment">Hufig wird der Ausdruck 'Blutdruck' zur Bezeichung der Messung des brachialen Ateriendrucks im Oberarm verwendet.</items>
                </term_definitions>
                <term_definitions code="deg">
                    <items id="text"></items>
                    <items id="description">degree</items>
                </term_definitions>
                <term_definitions code="mm[Hg]">
                    <items id="text">mmHg</items>
                    <items id="description">millimeters of mercury</items>
                </term_definitions>
